{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "api": {
        "ClusterId": "api",
        "Match": {
          "Path": "api/{**catch-all}"
        }
      },
      "frontend": {
        "ClusterId": "frontend",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "api": {
        "Destinations": {
          "api": {
            "Address": "http://localhost:5130/"
          }
        }
      },
      "frontend": {
        "Destinations": {
          "frontend": {
            "Address": "https://localhost:5173/"
          }
        }
      }
    }
  }
}